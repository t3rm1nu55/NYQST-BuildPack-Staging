{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "NYQST Run Event Envelope",
  "type": "object",
  "required": [
    "contract_version",
    "run_id",
    "sequence_num",
    "timestamp",
    "event_type",
    "payload"
  ],
  "properties": {
    "contract_version": {
      "type": "string",
      "const": "1.0.0"
    },
    "run_id": {
      "type": "string"
    },
    "sequence_num": {
      "type": "integer",
      "minimum": 1
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "event_type": {
      "type": "string"
    },
    "payload": {
      "type": "object"
    }
  }
}